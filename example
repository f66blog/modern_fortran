   "source": [
    "%fcflags: -O2\n",
    "program optimization_2\n",
    "    implicit none\n",
    "    real, parameter :: pi = 4.0 * atan(1.0)\n",
    "    real :: zeta4\n",
    "    integer :: i\n",
    "    zeta4 = 0.0\n",
    "    do i = 1, 10**2\n",
    "        zeta4 = zeta4 + 1.0 / real(i**4) \n",
    "    end do\n",
    "    print *, 'Sum_1..100 1/x^4 =', zeta4\n",
    "    print *, 'pi^4/90          =', pi**4 / 90.0 \n",
    "    stop 'normal end'\n",
    "end program optimization_2"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "\n",
    "$\n",
    "\\zeta(4)=\\sum_{k\\, =1}^\\infty{1\\over k^4}={\\pi^4\\over90}\n",
    "$"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## separate module compilation\n",
    "%module: *objectname*"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "[gfort kernel] module objects created successfully: mod.o"
     ]
    }
   ],
   "source": [
    "%module: mod\n",
    "%fcflags: -O2\n",
    "module m_sort\n",
    "    implicit none\n",
    "contains\n",
    "    recursive function qsort(x) result(res)\n",
    "        real, intent(in) :: x(:)\n",
    "        real             :: res(size(x))\n",
    "        if (size(x) == 0) return\n",
    "        res = [qsort( pack(x(2:), x(2:) <= x(1)) ), x(1), &\n",
    "               qsort( pack(x(2:), x(2:) >  x(1)) ) ]\n",
    "    end function qsort\n",
    "end module m_sort"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
