   "source": [
    "%fcflags: mod.o -O2\n",
    "program quicksort\n",
    "    use m_sort\n",
    "    implicit none\n",
    "    integer, parameter :: n = 50\n",
    "    real :: x(n)\n",
    "    call random_seed()\n",
    "    call random_number(x)\n",
    "    print '(10f7.4)', x\n",
    "    print *\n",
    "    print '(10f7.4)', qsort(x)\n",
    "end program quicksort"
   ]
  },
